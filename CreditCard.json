{
    "openapi": "3.0.0",
    "info": {
      "title": "Credit Card API",
      "version": "1.0.0",
      "description": "API for Credit Card Details and New Credit Card Requests"
    },
    "paths": {
      "/getCreditCardDetails": {
        "post": {
          "summary": "Get Credit Card Details",
          "requestBody": {
            "content": {
              "application/json": {
                "example": {
                  "CreditCardDetailsRequest": {
                    "cardNumber": "1234567812345678",
                    "cardType": "Visa"
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "example": {
                    "accno": 1,
                    "name": "John Doe",
                    "age": 30,
                    "phonenumber": "1234567890",
                    "dob": "1992-05-15",
                    "city": "New York",
                    "pincode": "10001",
                    "cardnumber": "1234567812345678",
                    "cardtype": "Visa",
                    "creditlimit": 5000,
                    "holdername": "John Doe",
                    "status": "Active",
                    "cibilscore": 750,
                    "branchno": 101,
                    "deliverymethod": "Standard",
                    "employeeid": 1001,
                    "salary": 60000
                  }
                }
              }
            }
          }
        }
      },
      "/addNewCreditCardDetails": {
        "post": {
          "summary": "Add New Credit Card Details",
          "requestBody": {
            "content": {
              "application/json": {
                "example": {
                  "NewCreditCardDetails": {
                    "NewCustomer": {
                      "accNo": 100021,
                      "name": "surya siddhi sai",
                      "age": 22,
                      "dob": "2001-10-27",
                      "AddressType": {
                        "city": "hyderabad",
                        "pincode": 500019
                      },
                      "Carddetails": {
                        "cardNumber": "12222924",
                        "cardType": "visa",
                        "branchNo": 21,
                        "creditLimit": 5000,
                        "holderName": "surya",
                        "status": "active",
                        "cibilScore": 700,
                        "deliveryMethod": "standard",
                        "employeeId": 123,
                        "salary": 20000
                      }
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "example": {
                    "NewCreditCardResponse": {
                      "accNo": 100021,
                      "cardNumber": "12222924",
                      "cardType": "visa",
                      "status": "active",
                      "response": "success"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  